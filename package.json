{
  "name": "typescript-project-template",
  "version": "1.2.0",
  "description": "A repo to provide a template about TypeScript lint &amp; format.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "pretty": "pretty-quick --staged",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjayheaven/typescript-project-template.git"
  },
  "author": "",
