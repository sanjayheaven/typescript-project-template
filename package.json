  "bugs": {
    "url": "https://github.com/sanjayheaven/typescript-project-template/issues"
  },
  "homepage": "https://github.com/sanjayheaven/typescript-project-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.3"
